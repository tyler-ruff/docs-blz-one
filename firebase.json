{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "docs-blz-one",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "frameworksBackend": {
      "region": "us-east1",
      "type": "vitepress"
    }
  },
  "storage": {
    "rules": "storage.rules"
  }
}
